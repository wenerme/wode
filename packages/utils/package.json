{
  "name": "@wener/utils",
  "version": "1.1.14",
  "type": "module",
  "description": "Utils for daily use",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wener/wode.git"
  },
  "homepage": "https://github.com/wenerme/wode#readme",
  "author": "wener",
  "license": "MIT",
  "main": "dist/cjs/index.cjs",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "typescript": "./src/index.ts",
      "system": "./dist/system/index.js",
      "import": "./lib/index.js"
    },
    "./types": {
      "types": "./types.d.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "typescript": "./src/server.ts",
      "system": "./dist/system/server.js",
      "import": "./lib/server.js"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "types": "src/index.ts",
  "files": [
    "dist",
    "index.ts",
    "lib",
    "server.ts",
    "src",
    "tsconfig.json"
  ],
  "keywords": [
    "utils",
    "core library",
    "helper",
    "camelcase",
    "get",
    "set",
    "template",
    "lodash"
  ],
  "scripts": {
    "build": "make prepublish",
    "clean": "make clean",
    "dev": "make dev",
    "test": "make test"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.194",
    "@types/ws": "^8.5.4",
    "https-proxy-agent": "^6.1.0",
    "lodash": "^4.17.21"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "peerDependencies": {
    "https-proxy-agent": "*",
    "undici": "*"
  },
  "peerDependenciesMeta": {
    "undici": {
      "optional": true
    },
    "https-proxy-agent": {
      "optional": true
    }
  },
  "sideEffects": false,
  "rollup": {
    "input": [
      "./src/index.ts",
      "./src/server.ts"
    ],
    "external": [
      "jsdom",
      "node-fetch"
    ]
  },
  "dependencies": {
    "undici": "^5.22.1"
  }
}
