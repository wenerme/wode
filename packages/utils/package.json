{
  "name": "@wener/utils",
  "version": "1.1.19",
  "type": "module",
  "description": "Utils for daily use",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wener/wode.git"
  },
  "homepage": "https://github.com/wenerme/wode#readme",
  "author": "wener",
  "license": "MIT",
  "files": [
    "dist",
    "index.ts",
    "lib",
    "server.ts",
    "src",
    "tsconfig.json"
  ],
  "keywords": [
    "utils",
    "core library",
    "helper",
    "camelcase",
    "get",
    "set",
    "template",
    "lodash"
  ],
  "scripts": {
    "build": "make prepublish",
    "clean": "make clean",
    "dev": "make dev",
    "test": "make test"
  },
  "peerDependencies": {
    "undici": "*"
  },
  "peerDependenciesMeta": {
    "undici": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/lodash": "^4.14.196",
    "@types/ws": "^8.5.5",
    "https-proxy-agent": "^7.0.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "undici": "^5.23.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "exports": {
      ".": {
        "types": "./src/index.ts",
        "typescript": "./src/index.ts",
        "system": "./dist/system/index.js",
        "import": "./lib/index.js"
      },
      "./types": {
        "types": "./types.d.ts"
      },
      "./server": {
        "types": "./src/server.ts",
        "typescript": "./src/server.ts",
        "system": "./dist/system/server.js",
        "import": "./lib/server.js"
      },
      "./src/*": "./src/*",
      "./package.json": "./package.json"
    }
  },
  "sideEffects": false,
  "rollup": {
    "input": [
      "./src/index.ts",
      "./src/server.ts"
    ],
    "external": [
      "crypto",
      "jsdom",
      "node-fetch",
      "undici"
    ]
  }
}
