{
  "name": "@wener/utils",
  "version": "1.1.11",
  "type": "module",
  "description": "Utils for daily use",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wener/wode.git"
  },
  "homepage": "https://github.com/wenerme/wode#readme",
  "author": "wener",
  "license": "MIT",
  "main": "dist/cjs/index.cjs",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "typescript": "./src/index.ts",
      "import": "./lib/index.js",
      "system": {
        "default": "./dist/system/index.js"
      },
      "require": "./dist/cjs/index.cjs"
    },
    "./types": {
      "types": "./types.d.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "typescript": "./src/server.ts",
      "import": "./lib/server.js",
      "system": {
        "default": "./dist/system/server.js"
      },
      "require": "./dist/cjs/server.cjs"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "types": "src/index.ts",
  "files": [
    "dist",
    "index.ts",
    "lib",
    "server.ts",
    "src",
    "tsconfig.json"
  ],
  "keywords": [
    "utils",
    "core library",
    "helper",
    "camelcase",
    "get",
    "set",
    "template",
    "lodash"
  ],
  "scripts": {
    "build": "make prepublish",
    "clean": "make clean",
    "dev": "make dev",
    "test": "make test"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/ws": "^8.5.4",
    "ava": "^5.1.1",
    "https-proxy-agent": "^5.0.1",
    "lodash": "^4.17.21",
    "tsd": "^0.25.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "sideEffects": false,
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--conditions=typescript",
      "--experimental-import-meta-resolve",
      "--require=@wener/wode/suppress-experimental.cjs",
      "--loader=tsx"
    ]
  },
  "rollup": {
    "input": [
      "./src/index.ts",
      "./src/server.ts"
    ],
    "external": [
      "jsdom",
      "node-fetch"
    ]
  },
  "typedoc": {
    "entryPoint": "./src/typedoc.ts",
    "readmeFile": "./README.md"
  }
}
