{
  "name": "@wener/utils",
  "version": "1.1.2",
  "description": "Utils for daily use",
  "type": "module",
  "sideEffects": false,
  "keywords": [
    "utils",
    "core library",
    "helper"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wener/wode.git"
  },
  "license": "MIT",
  "author": "wener",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./lib/esm/index.mjs",
      "system": {
        "development": "./dist/system/index.development.js",
        "default": "./dist/system/index.min.js"
      },
      "require": "./dist/cjs/index.js"
    },
    "./types": {
      "types": "./src/types.d.ts"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "import": "lib/esm/index.mjs",
  "types": "src/index.ts",
  "homepage": "https://github.com/wenerme/wode#readme",
  "files": [
    "index.js",
    "index.d.ts",
    "lib",
    "dist",
    "src",
    "README.md",
    "rollup.config.ts",
    "jest.config.js",
    "jest.setup.js",
    "tsconfig.rollup.json",
    "tsconfig.jest.json",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "make prepublish",
    "clean": "make clean",
    "dev": "make dev",
    "test": "make test"
  },
  "devDependencies": {
    "@types/lodash": "^4",
    "@types/node": "^16",
    "ava": "^4.3.3",
    "dotenv": "^16",
    "esbuild": "^0.14",
    "lodash": "^4",
    "tslib": "^2",
    "typescript": "^4"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx"
    ]
  }
}
