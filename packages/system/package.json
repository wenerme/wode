{
  "name": "@wener/system",
  "version": "1.0.6",
  "type": "module",
  "description": "Utils for systemjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wener/wode.git"
  },
  "homepage": "https://github.com/wenerme/wode#readme",
  "author": "wener",
  "license": "MIT",
  "engines": {
    "node": "16||18",
    "npm": "8"
  },
  "main": "dist/cjs/index.js",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "typescript": "./src/index.ts",
      "import": "./lib/index.js",
      "system": {
        "default": "./dist/system/index.js"
      },
      "require": "./dist/cjs/index.cjs"
    },
    "./types": {
      "types": "./src/types.d.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "typescript": "./src/server.ts",
      "import": "./lib/server.js",
      "system": {
        "default": "./dist/system/server.js"
      },
      "require": "./dist/cjs/server.cjs"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "types": "src/index.ts",
  "files": [
    "dist",
    "index.ts",
    "lib",
    "server.ts",
    "src",
    "tsconfig.json"
  ],
  "keywords": [
    "utils",
    "system",
    "helper"
  ],
  "scripts": {
    "build": "make prepublish",
    "clean": "make clean",
    "dev": "make dev",
    "test": "make test"
  },
  "peerDependencies": {
    "node-fetch": "*",
    "systemjs": "*"
  },
  "peerDependenciesMeta": {
    "node-fetch": {
      "optional": true
    },
    "systemjs": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "node-fetch": "*",
    "systemjs": "*"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@wener/utils": "workspace:^1.1.8",
    "ava": "^5.1.0",
    "jsdom": "^20.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "sideEffects": false,
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--conditions=typescript",
      "--require=@wener/wode/suppress-experimental.cjs",
      "--loader=tsx"
    ]
  },
  "rollup": {
    "input": [
      "./src/index.ts",
      "./src/server.ts"
    ]
  },
  "typedoc": {
    "entryPoint": "./src/typedoc.ts",
    "readmeFile": "./README.md"
  }
}
