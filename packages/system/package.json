{
  "name": "@wener/system",
  "version": "1.0.1",
  "type": "module",
  "description": "Utils for systemjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wener/wode.git"
  },
  "homepage": "https://github.com/wenerme/wode#readme",
  "author": "wener",
  "license": "MIT",
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "main": "dist/cjs/index.js",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./lib/index.js",
      "system": {
        "default": "./dist/system/index.js"
      },
      "require": "./dist/cjs/index.js"
    },
    "./types": {
      "types": "./src/types.d.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./lib/server.js",
      "system": {
        "default": "./dist/system/server.js"
      },
      "require": "./dist/cjs/server.js"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "types": "src/index.ts",
  "files": [
    "dist",
    "index.ts",
    "lib",
    "server.ts",
    "src",
    "tsconfig.json"
  ],
  "keywords": [
    "utils",
    "system",
    "helper"
  ],
  "scripts": {
    "build": "make prepublish",
    "clean": "make clean",
    "dev": "make dev",
    "test": "make test"
  },
  "devDependencies": {
    "@wener/utils": "^1",
    "ava": "^4.3.3"
  },
  "peerDependencies": {
    "systemjs": "*",
    "node-fetch": "*"
  },
  "peerDependenciesMeta": {
    "systemjs": {
      "optional": true
    },
    "node-fetch": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "systemjs": "*",
    "node-fetch": "*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "sideEffects": false,
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx"
    ]
  },
  "rollup": {
    "input": [
      "./src/index.ts",
      "./src/server.ts"
    ]
  }
}
