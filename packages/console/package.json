{
  "name": "@wener/console",
  "version": "1.0.5",
  "type": "module",
  "description": "Base console UI toolkit",
  "author": "wener",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./lib/index.js"
    },
    "./valtio": {
      "types": "./valtio.ts",
      "default": "./lib/components/valtio/index.js"
    },
    "./buildinfo": {
      "types": "./buildinfo.ts",
      "default": "./lib/buildinfo/index.js"
    },
    "./loader": {
      "types": "./loader.ts",
      "default": "./lib/components/loader/index.js"
    },
    "./components": {
      "types": "./components.ts",
      "default": "./lib/components/index.js"
    },
    "./icons": {
      "types": "./icons.ts",
      "default": "./lib/icons/index.js"
    },
    "./web": {
      "types": "./web.ts",
      "default": "./lib/web/index.js"
    },
    "./web/user": {
      "types": "./web/user.ts",
      "default": "./lib/web/user/index.js"
    },
    "./pages": {
      "types": "./web/pages.ts",
      "default": "./lib/web/pages/index.js"
    },
    "./toast": {
      "types": "./toast.ts",
      "default": "./lib/components/toast/index.js"
    },
    "./web/pages/setting/about/route": {
      "types": "./web/pages/setting/about/route.ts",
      "default": "./lib/web/pages/setting/about/route.js"
    },
    "./web/pages/setting/appearance/route": {
      "types": "./web/pages/setting/appearance/route.ts",
      "default": "./lib/web/pages/setting/appearance/route.js"
