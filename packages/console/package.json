{
  "name": "@wener/console",
  "version": "1.0.1",
  "type": "module",
  "description": "Base console UI toolkit",
  "author": "wener",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./lib/index.js"
    },
    "./valtio": {
      "types": "./valtio.ts",
      "default": "./lib/components/valtio/index.js"
    },
    "./buildinfo": {
      "types": "./buildinfo.ts",
      "default": "./lib/buildinfo/index.js"
    },
    "./loader": {
      "types": "./loader.ts",
      "default": "./lib/components/loader/index.js"
    },
    "./icons": {
      "types": "./icons.ts",
      "default": "./lib/icons/index.js"
    },
    "./console": {
      "types": "./console.ts",
      "default": "./lib/components/console/index.js"
    },
    "./console/user": {
      "types": "./console/user.ts",
      "default": "./lib/components/console/user/index.js"
    },
    "./pages": {
      "types": "./console/pages.ts",
      "default": "./lib/components/console/pages/index.js"
    },
    "./toast": {
      "types": "./console/pages.ts",
      "default": "./lib/components/toast/index.js"
    },
    "./console/pages/setting/about/route": {
      "types": "./src/components/console/pages/setting/about/route.ts",
      "default": "./lib/components/console/pages/setting/about/route.js"
    },
    "./console/pages/setting/appearance/route": {
      "types": "./src/components/console/pages/setting/appearance/route.ts",
      "default": "./lib/components/console/pages/setting/appearance/route.js"
    },
    "./console/pages/setting/dev/debug/route": {
      "types": "./src/components/console/pages/setting/dev/debug/route.ts",
      "default": "./lib/components/console/pages/setting/dev/debug/route.js"
    },
    "./daisy": {
      "types": "./daisy.ts",
      "default": "./lib/components/daisy/index.js"
    },
    "./router": {
      "types": "./router.ts",
      "default": "./lib/components/router/index.js"
    },
    "./floating": {
      "types": "./floating.ts",
      "default": "./lib/components/floating/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "*.ts",
    "components/**/*.ts",
    "console/**/*.ts",
    "lib",
    "postcss.config.cjs",
    "src",
    "tailwind.config.mjs"
  ],
  "keywords": [
    "admin",
    "console",
    "react",
    "daisy",
    "tailwindcss",
    "builder"
  ],
  "scripts": {
    "build": "make build",
    "deploy": "make deploy",
    "test": "make test"
  },
  "peerDependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@floating-ui/react": "^0.26.0",
    "@headlessui/react": "^1.7.17",
    "@tanstack/react-query": "^4.35.3",
    "@trpc/client": "^10.38.4",
    "@trpc/react-query": "^10.38.4",
    "@wener/utils": "workspace:^",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6",
    "superjson": "^2",
    "use-immer": "^0.9.0",
    "valtio": "^1.11.2",
    "zod": "^3.22.2",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@floating-ui/react": "^0.26.2",
    "@headlessui/react": "^1.7.17",
    "@nestjs/common": "^10.2.10",
    "@tailwindcss/forms": "^0.5.7",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@wener/nestjs": "workspace:^",
    "@wener/reaction": "workspace:^",
    "@wener/utils": "workspace:^",
    "clsx": "^2.0.0",
    "immer": "^10.0.3",
    "next": "^14.0.3",
    "react-hook-form": "^7.48.2",
    "react-icons": "^4.12.0"
  },
  "sideEffects": false
}
