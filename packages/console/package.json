    "./web/pages/setting/dev/debug/route": {
      "types": "./web/pages/setting/dev/debug/route.ts",
      "default": "./lib/web/pages/setting/dev/debug/route.js"
    },
    "./daisy": {
      "types": "./daisy.ts",
      "default": "./lib/components/daisy/index.js"
    },
    "./router": {
      "types": "./router.ts",
      "default": "./lib/components/router/index.js"
    },
    "./floating": {
      "types": "./floating.ts",
      "default": "./lib/components/floating/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "*.ts",
    "components/**/*.ts",
    "lib",
    "postcss.config.cjs",
    "src",
    "tailwind.config.mjs",
    "web/**/*.ts"
  ],
  "keywords": [
    "admin",
    "console",
    "react",
    "daisy",
    "tailwindcss",
    "builder"
  ],
  "scripts": {
    "build": "make build",
    "deploy": "make deploy",
    "test": "make test"
  },
  "peerDependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@floating-ui/react": "^0.26.0",
    "@headlessui/react": "^1.7.17",
    "@tanstack/react-query": "^4.35.3",
    "@trpc/client": "^10.38.4",
    "@trpc/react-query": "^10.38.4",
    "@wener/utils": "workspace:^",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6",
    "superjson": "^2",
    "use-immer": "^0.9.0",
    "valtio": "^1.11.2",
    "zod": "^3.22.2",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.6.3",
    "@floating-ui/react": "^0.26.9",
    "@headlessui/react": "^1.7.18",
    "@nestjs/common": "^10.3.3",
    "@remix-run/router": "^1.15.1",
    "@tailwindcss/forms": "^0.5.7",
    "@wener/nestjs": "workspace:^",
    "@wener/reaction": "workspace:^",
    "@wener/utils": "workspace:^",
    "clsx": "^2.1.0",
    "immer": "^10.0.3",
    "next": "^14.1.0",
    "react-hook-form": "^7.50.1",
    "react-icons": "^5.0.1"
  },
  "sideEffects": false
}
