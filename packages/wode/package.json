{
  "name": "@wener/wode",
  "version": "1.0.0",
  "type": "module",
  "description": "Wener's node starter conf",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wenerme/wode.git"
  },
  "homepage": "https://github.com/wenerme/wode#readme",
  "bugs": {
    "url": "https://github.com/wenerme/wode/issues"
  },
  "author": "wener",
  "license": "MIT",
  "packageManager": "pnpm@7.12.0+",
  "files": [
    "*.cjs",
    "*.mjs",
    "tsconfig",
    "eslint-config",
    "Makefile",
    "node.mk"
  ],
  "keywords": [
    "wener",
    "prettier",
    "eslint",
    "rollup",
    "suppress experimental warning"
  ],
  "scripts": {},
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--require=@wener/wode/suppress-experimental.cjs",
      "--loader=tsx"
    ]
  }
}
