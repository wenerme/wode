{
  "name": "@wener/unpkg",
  "version": "1.0.6",
  "type": "module",
  "description": "NPM Registry unpkg",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wener/wode.git"
  },
  "homepage": "https://github.com/wenerme/wode#readme",
  "author": "wener",
  "license": "MIT",
  "main": "dist/cjs/index.cjs",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "typescript": "./src/index.ts",
      "import": "./lib/index.js",
      "system": {
        "default": "./dist/system/index.js"
      },
      "require": "./dist/cjs/index.cjs"
    },
    "./types": {
      "types": "./types.d.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "typescript": "./src/server.ts",
      "import": "./lib/server.js",
      "system": {
        "default": "./dist/system/server.js"
      },
      "require": "./dist/cjs/server.cjs"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "types": "src/index.ts",
  "files": [
    "dist",
    "index.ts",
    "lib",
    "server.ts",
    "src",
    "tsconfig.json"
  ],
  "keywords": [
    "unpkg",
    "registry",
    "jsdelivr",
    "npm",
    "cdn"
  ],
  "scripts": {
    "build": "make prepublish",
    "clean": "make clean",
    "dev": "make dev",
    "test": "make test"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/fs-extra": "^11.0.4",
    "@types/semver": "^7.5.8",
    "@types/tar": "^6.1.13"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "sideEffects": false,
  "rollup": {
    "input": [
      "./src/index.ts",
      "./src/server.ts"
    ],
    "external": [
      "better-sqlite3",
      "mime",
      "tar",
      "pino",
      "semver",
      "lru-cache",
      "node-fetch"
    ]
  },
  "typedoc": {
    "entryPoint": "./src/typedoc.ts",
    "readmeFile": "./README.md"
  },
  "dependencies": {
    "@wener/utils": "*",
    "better-sqlite3": "^11.1.2",
    "lru-cache": "^11.0.0",
    "mime": "^4.0.4",
    "pino": "^9.3.2",
    "semver": "^7.6.3",
    "tar": "^7.4.3"
  },
  "optionalDependencies": {
    "@wener/utils": "workspace:^"
  }
}
