{
  "name": "@wener/reaction",
  "version": "1.1.1",
  "description": "React hooks & reaction",
  "keywords": [
    "components",
    "react",
    "hooks",
    "util",
    "tool"
  ],
  "scripts": {
    "build": "make prepublish",
    "clean": "make clean",
    "dev": "make dev"
  },
  "bugs": {
    "url": "https://github.com/wenerme/wode/issues"
  },
  "homepage": "https://github.com/wenerme/wode#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wenerme/wode.git",
    "directory": "packages/reaction"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "src/index.ts",
  "umd:main": "dist/wener-reaction.js",
  "unpkg": "dist/wener-reaction.js",
  "jsdelivr": "dist/wener-reaction.js",
  "jsnext:main": "dist/wener-reaction.mjs",
  "esnext": "lib/esnext/index.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "types": "./src/index.ts",
      "require": "./lib/cjs/index.js"
    }
  },
  "files": [
    "src",
    "lib",
    "dist",
    "tsconfig.json",
    "Makefile"
  ],
  "devDependencies": {
    "@types/react": "^18",
    "esbuild": "^0.14",
    "react": "^18",
    "typescript": "^4"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "trailingComma": "all",
    "printWidth": 120,
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          "*.html",
          "*.css"
        ],
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
