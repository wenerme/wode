  },
  "peerDependenciesMeta": {
    "hoist-non-react-statics": {
      "optional": true
    },
    "prop-types": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "react-is": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
