{
  "name": "@wener/reaction",
  "version": "1.2.16",
  "description": "React hooks, utils & reaction",
  "keywords": [
    "components",
    "react",
    "hooks",
    "util",
    "tool"
  ],
  "homepage": "https://github.com/wenerme/wode#readme",
  "bugs": {
    "url": "https://github.com/wenerme/wode/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wenerme/wode.git",
    "directory": "packages/reaction"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "typescript": "./src/index.ts",
      "import": "./lib/index.js",
      "system": {
        "default": "./dist/system/index.js"
      },
      "require": "./dist/cjs/index.cjs"
    },
    "./types": {
      "types": "./types.d.ts"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.cjs",
  "import": "lib/index.js",
  "types": "src/index.ts",
  "files": [
    "src",
    "lib",
    "dist",
    "tsconfig.json",
    "Makefile"
  ],
  "scripts": {
    "build": "make prepublish",
    "clean": "make clean",
    "dev": "make dev"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all",
    "overrides": [
      {
        "files": [
          "*.html",
          "*.css"
        ],
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "dependencies": {
    "@wener/utils": "^1",
    "hoist-non-react-statics": "^3.3.2",
    "react-is": "^18.2.0"
  },
  "devDependencies": {
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/prop-types": "^15",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18",
    "@types/react-is": "^17.0.3"
  },
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md"
  }
}
