    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.cjs",
  "import": "lib/index.js",
  "types": "src/index.ts",
  "files": [
    "src",
    "lib",
    "dist",
    "tsconfig.json",
    "Makefile"
  ],
  "scripts": {
    "build": "make prepublish",
    "clean": "make clean",
    "dev": "make dev"
