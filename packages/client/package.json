{
  "name": "@wener/client",
  "version": "1.0.15",
  "type": "module",
  "description": "Client for Wechat, Wecom, Xunfei, etc.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wener/wode.git"
  },
  "homepage": "https://github.com/wenerme/wode#readme",
  "author": "wener",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./lib/index.js"
    },
    "./server": {
      "types": "./server.ts",
      "default": "./lib/server/index.js"
    },
    "./wechat": {
      "types": "./wechat.ts",
      "default": "./lib/wechat/index.js"
    },
    "./wechat/webhook": {
      "types": "./wechat/webhook.ts",
      "default": "./lib/wechat/webhook/index.js"
    },
    "./wecom": {
      "types": "./wecom.ts",
      "default": "./lib/wecom/index.js"
    },
    "./wecom/server": {
      "types": "./wecom/server.ts",
      "default": "./lib/wecom/server/index.js"
    },
    "./wecom/webhook": {
      "types": "./wecom/webhook.ts",
      "default": "./lib/wecom/webhook/index.js"
    },
    "./xunfei/spark": {
      "types": "./src/xunfei/spark/index.ts",
      "default": "./lib/xunfei/spark/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "*.js",
    "*.ts",
    "lib",
    "src",
    "wechat/*.ts",
    "wecom/*.ts",
    "xunfei/*.ts",
    "xunfei/spark/*.ts"
  ],
  "keywords": [],
  "scripts": {
    "build": "make build",
    "deploy": "make deploy",
    "test": "make test"
  },
  "dependencies": {
    "@wener/utils": "workspace:^",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@wecom/jssdk": "^1.4.3",
    "fast-xml-parser": "^4.3.2",
    "undici": "^5.25.4",
    "ws": "^8.14.2"
  }
}
