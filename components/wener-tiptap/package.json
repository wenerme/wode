{
  "name": "@wener/tiptap",
  "version": "1.0.0",
  "description": "Collection of useful tiptap extensions",
  "main": "lib/esm/index.js",
  "types": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "dist/wener-tiptap.cjs.js",
    "umd": "dist/wener-tiptap.umd.js",
    "module": "dist/wener-tiptap.esm.js",
    "types": "lib/esm/index.d.ts"
  },
  "scripts": {
    "test": "jest",
    "build": "make build",
    "dev": "esbuild --watch --format=esm --outdir=lib/esm --target=chrome90 src/index.ts src/**/*.ts",
    "lint": "tsc --noEmit",
    "clean": "rm -rf lib/*"
  },
  "keywords": [
    "tiptap",
    "editor",
    "extension",
    "prosemirror"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wener/wode.git"
  },
  "license": "MIT",
  "author": "wener",
  "files": [
    "lib",
    "dist",
    "src",
    "README.md",
    "rollup.config.ts",
    "jest.config.js",
    "jest.setup.js",
    "tsconfig.json"
  ],
  "dependencies": {
    "@tiptap/extension-bold": "2.0.0-beta.26",
    "@tiptap/extension-character-count": "^2.0.0-beta.26",
    "@tiptap/extension-color": "^2.0.0-beta.9",
    "@tiptap/extension-font-family": "^2.0.0-beta.21",
    "@tiptap/extension-highlight": "^2.0.0-beta.33",
    "@tiptap/extension-horizontal-rule": "^2.0.0-beta.31",
    "@tiptap/extension-image": "^2.0.0-beta.27",
    "@tiptap/extension-italic": "2.0.0-beta.26",
    "@tiptap/extension-link": "^2.0.0-beta.38",
    "@tiptap/extension-paragraph": "2.0.0-beta.23",
    "@tiptap/extension-placeholder": "^2.0.0-beta.48",
    "@tiptap/extension-strike": "2.0.0-beta.27",
    "@tiptap/extension-table": "^2.0.0-beta.49",
    "@tiptap/extension-table-cell": "^2.0.0-beta.20",
    "@tiptap/extension-table-header": "^2.0.0-beta.22",
    "@tiptap/extension-table-row": "^2.0.0-beta.19",
    "@tiptap/extension-task-item": "^2.0.0-beta.32",
    "@tiptap/extension-task-list": "^2.0.0-beta.26",
    "@tiptap/extension-text-align": "^2.0.0-beta.29",
    "@tiptap/extension-text-style": "^2.0.0-beta.23",
