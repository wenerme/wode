{
  "name": "@wener/tiptap",
  "version": "1.0.0",
  "description": "Collection of useful tiptap extensions",
  "type": "module",
  "sideEffects": false,
  "types": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./lib/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/wener-tiptap.cjs.js",
    "umd": "dist/wener-tiptap.umd.js",
    "module": "dist/wener-tiptap.esm.js",
    "types": "lib/esm/index.d.ts"
  },
  "scripts": {
    "test": "make test",
    "build": "make build",
    "dev": "esbuild --watch --format=esm --outdir=lib/esm --target=chrome90 src/index.ts src/**/*.ts",
    "lint": "tsc --noEmit",
    "clean": "rm -rf lib/*"
  },
  "keywords": [
    "tiptap",
    "editor",
    "extension",
    "prosemirror"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wener/wode.git"
  },
  "license": "MIT",
  "author": "wener",
  "files": [
    "lib",
    "dist",
    "src",
    "types",
    "README.md",
    "rollup.config.js",
    "tsconfig.json"
  ],
  "dependencies": {
    "@tiptap/core": "2.0.0-beta.209",
    "@tiptap/extension-blockquote": "2.0.0-beta.209",
    "@tiptap/extension-bold": "2.0.0-beta.209",
    "@tiptap/extension-bubble-menu": "2.0.0-beta.202",
    "@tiptap/extension-bullet-list": "2.0.0-beta.209",
    "@tiptap/extension-character-count": "2.0.0-beta.209",
    "@tiptap/extension-code": "2.0.0-beta.209",
    "@tiptap/extension-code-block": "2.0.0-beta.209",
    "@tiptap/extension-collaboration": "2.0.0-beta.202",
    "@tiptap/extension-collaboration-cursor": "2.0.0-beta.202",
    "@tiptap/extension-color": "2.0.0-beta.209",
    "@tiptap/extension-document": "2.0.0-beta.209",
    "@tiptap/extension-dropcursor": "2.0.0-beta.209",
    "@tiptap/extension-floating-menu": "2.0.0-beta.202",
    "@tiptap/extension-focus": "2.0.0-beta.202",
    "@tiptap/extension-font-family": "2.0.0-beta.209",
    "@tiptap/extension-gapcursor": "2.0.0-beta.202",
    "@tiptap/extension-hard-break": "2.0.0-beta.209",
    "@tiptap/extension-heading": "2.0.0-beta.209",
    "@tiptap/extension-highlight": "2.0.0-beta.209",
    "@tiptap/extension-history": "2.0.0-beta.209",
    "@tiptap/extension-horizontal-rule": "2.0.0-beta.209",
    "@tiptap/extension-image": "2.0.0-beta.209",
    "@tiptap/extension-italic": "2.0.0-beta.209",
    "@tiptap/extension-link": "2.0.0-beta.209",
    "@tiptap/extension-list-item": "2.0.0-beta.202",
    "@tiptap/extension-mention": "2.0.0-beta.202",
    "@tiptap/extension-ordered-list": "2.0.0-beta.209",
    "@tiptap/extension-paragraph": "2.0.0-beta.209",
    "@tiptap/extension-placeholder": "2.0.0-beta.209",
    "@tiptap/extension-strike": "2.0.0-beta.209",
    "@tiptap/extension-subscript": "2.0.0-beta.202",
    "@tiptap/extension-superscript": "2.0.0-beta.202",
    "@tiptap/extension-table": "2.0.0-beta.209",
    "@tiptap/extension-table-cell": "2.0.0-beta.209",
    "@tiptap/extension-table-header": "2.0.0-beta.209",
    "@tiptap/extension-table-row": "2.0.0-beta.209",
    "@tiptap/extension-task-item": "2.0.0-beta.209",
    "@tiptap/extension-task-list": "2.0.0-beta.209",
    "@tiptap/extension-text": "2.0.0-beta.209",
    "@tiptap/extension-text-align": "2.0.0-beta.209",
    "@tiptap/extension-text-style": "2.0.0-beta.209",
    "@tiptap/extension-typography": "2.0.0-beta.202",
    "@tiptap/extension-underline": "2.0.0-beta.209",
    "@tiptap/html": "2.0.0-beta.209",
    "@tiptap/react": "2.0.0-beta.209",
    "@tiptap/starter-kit": "2.0.0-beta.209",
    "@tiptap/suggestion": "2.0.0-beta.209",
    "@wener/utils": "workspace:*",
    "classnames": "^2.3.2",
    "linkify-it": "^4.0.1",
    "markdown-it": "^13.0.1",
    "mdurl": "^1.0.1",
    "prosemirror-markdown": "^1.10.1",
    "prosemirror-model": "^1.19.0",
    "prosemirror-state": "^1.4.2",
    "prosemirror-transform": "^1.7.1",
    "prosemirror-view": "^1.30.0",
    "y-prosemirror": "^1.2.0",
    "y-protocols": "^1.0.5",
    "yjs": "^13.5.44"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "y-prosemirror": ">=1",
    "y-protocols": ">=1",
    "yjs": ">=13"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "rollup": {},
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx"
    ]
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md"
  }
}
